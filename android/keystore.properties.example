# Android Keystore Properties Example
# Copy this file to keystore.properties and fill in your actual values
# DO NOT commit the real keystore.properties file to version control

# Path to your keystore file (relative to android/ directory)
# Example: keystore/my-release-key.keystore or ../keystore/my-release-key.keystore
storeFile=keystore/my-release-key.keystore

# Keystore password
storePassword=your_keystore_password

# Key alias name
keyAlias=my-key-alias

# Key password
keyPassword=your_key_password