{
	"appId": "com.fastnow.zenith",
	"appName": "Fast Now Zenith",
	"webDir": "dist",
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 2000,
			"launchAutoHide": true,
			"backgroundColor": "#000000",
			"androidSplashResourceName": "splash",
			"androidScaleType": "CENTER_CROP",
			"showSpinner": false
		},
		"StatusBar": {
			"style": "dark"
		}
	},
	"android": {
		"allowMixedContent": true,
		"captureInput": true
	}
}
